      delay(100);// Wait for 500 milliseconds
      digitalWrite(WIFI_LED_PIN, LOW);// Turn the LED off
      digitalWrite(WIFI_DIS_LED_PIN, LOW);
      delay(100); // Wait for 500 milliseconds

    }
  }
  Serial.println("Connected to WiFi");
  // Display the logo
  oled.drawBitmap(0, 0, epd_bitmap_Wi_Fi_Logo, 128, 64, WHITE);  // Logo bitmap
  oled.display();
  delay(6000);
  oled.clearDisplay();
}

void loop() {
  // Read temperature and humidity from DHT11
  float temperature = dht.readTemperature();
  float humidity = dht.readHumidity();

  // Read soil moisture value
  int rawSoilMoistureValue = analogRead(SOIL_MOISTURE_PIN);
  int soilMoistureValue = map(rawSoilMoistureValue, 0, 4095, 0, 1000);

  // Check if DHT sensor failed to read
  if (isnan(temperature) || isnan(humidity)) {
    Serial.println("Failed to read from DHT sensor!");
    oled.clearDisplay();
    oled.setCursor(0, 0);
    oled.println("DHT Error!");
    oled.display();
    delay(2000);
    return;
  }

  // Debugging: Print values to Serial Monitor
  unsigned long currentMillis = millis();
  if (currentMillis - previousPrintMillis >= printInterval) {
    previousPrintMillis = currentMillis;
    // Display values on OLED
    oled.clearDisplay();
    oled.setCursor(0, 10);
    oled.printf("Temperature: %.2f C\n", temperature);
    oled.setCursor(0, 25);
    oled.printf("Humidity: %.2f %%\n", humidity);
    oled.setCursor(0, 40);
    oled.printf("Soil Moisture: %d\n", soilMoistureValue);
    oled.setCursor(0, 55);

    // Soil moisture threshold calibration
    //    int moistureThreshold = 600;  // Adjust based on your calibration
    if (soilMoistureValue < 900) {
      oled.println("Soil is Humid");
      digitalWrite(WET_SOIL_LED_PIN, HIGH);
      digitalWrite(DRY_SOIL_LED_PIN, LOW);
      digitalWrite(HIGH_WATER_LED_PIN, LOW);
    } else if (soilMoistureValue < 600) {
      oled.println("Sensor in the water");
      digitalWrite(HIGH_WATER_LED_PIN, HIGH);
      digitalWrite(WET_SOIL_LED_PIN, LOW);
      digitalWrite(DRY_SOIL_LED_PIN, LOW);
    } else {
      oled.println("Soil is Dry");
      digitalWrite(DRY_SOIL_LED_PIN, HIGH);
      digitalWrite(WET_SOIL_LED_PIN, LOW);
      digitalWrite(HIGH_WATER_LED_PIN, LOW);
    }
    oled.display();  // Update OLED display
  }

  // WiFi LED logic
  if (WiFi.status() == WL_CONNECTED) {
    digitalWrite(WIFI_LED_PIN, HIGH); // Turn on WiFi LED when connected
  } else {
    digitalWrite(WIFI_LED_PIN, LOW); // Turn off WiFi LED when disconnected
  }

  // WiFi Disconnect LED logic
  if (WiFi.status() != WL_CONNECTED) {
    oled.drawBitmap(0, 0, epd_bitmap_Wi_Fi_disconnected_logo, 128, 64, WHITE);
    oled.display();
    delay(3000);
    oled.clearDisplay();
    digitalWrite(WIFI_DIS_LED_PIN, HIGH); // Turn on WiFi LED when connected
    digitalWrite(WIFI_LED_PIN, LOW);
  } else {
    digitalWrite(WIFI_DIS_LED_PIN, LOW);
    digitalWrite(WIFI_LED_PIN, HIGH); // Turn off WiFi LED when disconnected
  }

  // Handle backend data transmission every 30 seconds
  if (currentMillis - previousMillis >= interval) {
    previousMillis = currentMillis;
    sendDataToBackend(temperature, humidity, soilMoistureValue);
    for (int i = 0; i < 5; i++) {  // Loop to blink the LED 10 times
      digitalWrite(DATA_LED_PIN, HIGH);  // Turn the LED on
      delay(200);// Wait for 500 milliseconds
      digitalWrite(DATA_LED_PIN, LOW);// Turn the LED off
      delay(200); // Wait for 500 milliseconds
    }
  }
}



